{
  "name": "ramdasauce",
  "description": "Helper utilities for Ramda.",
  "dependencies": {
    "ramda": "0.24.1"
  },
  "devDependencies": {
    "@semantic-release/git": "^7.0.5",
    "ava": "0.21.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "10.0.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-rollup": "^3.0.0",
    "np": "3.1.0",
    "nyc": "13.1.0",
    "rollup": "1.0.0",
    "rollup-plugin-babel": "4.2.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-ramda": "^1.0.5",
    "rollup-plugin-uglify": "6.0.0",
    "semantic-release": "^15.12.5",
    "standard": "12.0.1"
  },
  "files": [
    "dist/ramdasauce.js"
  ],
  "keywords": [
    "ramda",
    "sauce",
    "utilities"
  ],
  "contributors": [
    {
      "name": "Infinite Red, Inc.",
      "email": "npm@infinite.red"
    },
    {
      "name": "Steve Kellock",
      "email": "steve@kellock.ca"
    }
  ],
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "license": "MIT",
  "main": "./dist/ramdasauce.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/skellock/ramdasauce.git"
  },
  "scripts": {
    "build": "rm -rf dist && BABEL_ENV=production rollup -c",
    "coverage": "nyc ava",
    "lint": "standard lib/* test/* rollup.config.js",
    "test": "ava",
    "ci:publish": "yarn semantic-release",
    "semantic-release": "semantic-release"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "version": "2.1.3",
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": "package.json",
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
